doctype html
html
  head
    block title
      meta(charset='utf-8')
      meta(http-equiv='X-UA-Compatible' content='IE=edge,chrome=1')
      title= title
      meta(name="description" content="")
      meta(name="viewport" content="width=device-width, initial-scale=1")
      link(rel="apple-touch-icon" href="apple-touch-icon.png")
      
      // styles
      link(rel='stylesheet' href='/bootstrap/css/bootstrap.min.css')
      link(rel='stylesheet' href='/tether/css/tether.min.css')
      link(rel='stylesheet' href='/vendor/jquery-ui-1.12.1/jquery-ui.structure.min.css')
      link(rel='stylesheet' href='/vendor/jquery-ui-1.12.1/jquery-ui.theme.min.css')
      // link(rel="stylesheet" 
           href="//fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic")
      // link(rel="stylesheet" href="//fonts.googleapis.com/icon?family=Material+Icons")
      // link(rel="stylesheet" href='/vue-material/vue-material.css')
      link(rel='stylesheet' href='/styles/style.css')

  body
    .layout

      // navbar
      include includes/navbar.pug
      
      // alerts
      include includes/alerts.pug

      // content
      block content
      
      // scripts
      script(src='/jquery/jquery.min.js')
      script(src='/tether/js/tether.min.js')
      script(src='/bootstrap/js/bootstrap.min.js')
      script(src='/vue/vue.js')
      // script(src='/vue-material/vue-material.js')
      script(src='/moment/min/moment.min.js')
      script(src='/vendor/jquery-ui-1.12.1/jquery-ui.min.js')
      script(src='/scripts/script.js')